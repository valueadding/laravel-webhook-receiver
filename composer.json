{
    "name": "valueadding/laravel-webhook-receiver",
    "description": "A Laravel package that receives webhook logs from multiple applications with authentication and a React-based viewer",
    "type": "library",
    "license": "proprietary",
    "keywords": ["laravel", "logging", "webhook", "receiver", "log-viewer"],
    "authors": [
        {
            "name": "Mike Smit",
            "email": "m.smit@valueadding.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ValueAdding\\WebhookReceiver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ValueAdding\\WebhookReceiver\\WebhookReceiverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
